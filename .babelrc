{
  /**
   * Babel presets:
   *
   * @babel/preset-env is a smart preset that allows you to 
   * use the latest JavaScript without needing to micromanage 
   * which syntax transforms (and optionally, browser polyfills) 
   * are needed by your target environment(s). This both makes your 
   * life easier and JavaScript bundles smaller!
   * @see https://babeljs.io/docs/en/babel-preset-env
   */
  "presets": ["@babel/preset-env"],

  /**
   * Babel plugins:
   * @see https://babeljs.io/docs/en/babel-plugin-proposal-class-properties
   * @see https://babeljs.io/docs/en/babel-plugin-proposal-decorators
   * @see https://babeljs.io/docs/en/babel-plugin-proposal-private-methods
   * @see https://www.npmjs.com/package/babel-plugin-transform-private-properties (will be removed use regular # instead of @Private)
   */
  "plugins": [
    ["@babel/plugin-proposal-class-properties", { "loose": true }],
    ["@babel/plugin-proposal-decorators", { "legacy": true }],
    ["@babel/plugin-proposal-private-methods", { "loose": true }]
  ]
}